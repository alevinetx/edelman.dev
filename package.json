    "next": "10.0.7",
    "next-sanity": "^0.1.10",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@sanity/cli": "^2.4.0",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "lerna": "^3.22.1",
    "prettier": "^2.2.1",
    "typescript": "^4.2.2"
