  "private": true,
  "scripts": {
    "dev": "npx sanity-codegen && next",
    "build": "yarn run build:sanity && pwd && yarn run build:web",
    "build:web": "npx sanity-codegen && next build ",
    "start:web": "next start",
    "build:sanity": "cd studio  && sanity build ../public/studio -y && cd ..",
    "start:sanity": "cp .env ./studio/.env.development && cd studio && sanity start",
    "postinstall": "lerna bootstrap",
    "amplify:edelmandevalgoliawebhook": "cd amplify/backend/function/edelmandevalgoliawebhook/src && yarn && tsc -p ./tsconfig.json && cd -",
